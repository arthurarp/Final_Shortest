{
  "interaction": { 
    "multiselect": true,
    "hoverConnectedEdges": false
  },

  "autoResize": true,
  "height": "90%",
  "width": "100%",
  "nodes": {
    "borderWidth": 3,
    "size": 10,
    "color": {
      "border": "#FFF",
      "background": "#FFF",
      "highlight": {
        "border": "#FFF",
        "background": "#FFF"
      }
    },
    "font": { "color": "#2f2f2f" }
  },
  "edges": {
    "color": "#000",
    "arrows": {
      "to": {
        "enabled": true
      },
      "from": {
        "enabled": false
      }
    }
  }
}
